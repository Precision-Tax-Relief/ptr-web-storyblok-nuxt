{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "windev": "nuxt dev --port=3010",
    "dev": "nuxt dev",
    "dev-ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "typecheck": "nuxt typecheck",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm lint:js && npm lint:prettier",
    "lintfix": "prettier --write --list-different . && npm lint:js --fix",
    "proxy": "mkcert -install && mkcert localhost && local-ssl-proxy --source 3000 --target 3010 --cert localhost.pem --key localhost-key.pem",
    "pull-components": "node scripts/pull-components.mjs",
    "generate-sb-types": "storyblok-generate-ts source=./types/components.$SPACE_ID.json target=./types/component-types-sb"
  },
  "devDependencies": {
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxt/types": "^2.16.3",
    "@nuxt/typescript-build": "^3.0.1",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/google-fonts": "^3.0.1",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "autoprefixer": "^10.4.14",
    "consola": "^3.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.1",
    "nuxt": "^3.4.3",
    "nuxt-icon": "^0.4.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "storyblok-generate-ts": "^1.15.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.10",
    "@storyblok/nuxt": "^5.4.7",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/nuxt": "^10.1.2",
    "node-fetch": "^3.3.2",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-simple-sitemap": "^2.7.0",
    "nuxt-swiper": "^1.1.0",
    "pinia": "^2.0.36",
    "typescript": "^5.1.3",
    "vue-tsc": "^1.6.4"
  }
}
